services:
  work.web:
    build:
      context: E:\projects\learning\Work\Work
      dockerfile: Src/Work.Web/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Work.Web
      target: base
    container_name: Work.Web
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: workweb:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/Work.Web.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - E:\projects\learning\Work\Work\Src\Work.Web:/app:rw
    - C:\Users\ASUS\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\ASUS\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\ASUS\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\ASUS\.nuget\packages:/root/.nuget/packages:ro
    - E:\projects\learning\Work\Work:/src:rw
version: '3.4'